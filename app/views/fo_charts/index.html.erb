<div>
  <label>StockCode >
    <input id="stockcode"/>
  </label>
  <button id="data">Put/Call Data</button>
  <button id="ratio">Put/Call Ratio</button>
</div>
<div id="data_area"></div>
<script type="text/javascript">
    $(document).ready(function() {
        $("button").button();
        $("input#stockcode").autocomplete({
            source: [<%=raw @stocks.collect!{ |s| "'#{s.symbol}'" }.join(',') %>]
        });
        $("button").click(function() {
            var stock_id = $('#stockcode').val();
            $.get('/fo_charts/' + this.id + '/' +stock_id, function(data) {
                $('#data_area').html(data);
            });
            event.preventDefault();
        });
    });
</script>

